1582897379674:sumPartition <- function(dat,z){
1582897379675:k <-length(unique(z))
1582897379676:nobs <- tapply(1:nrow(dat),z,length)
1582897379677:withinSS <- tapply(1:nrow(dat),z,function(i){
1582897379678:if (length(i)==1) 0
1582897379679:else {x<- dat[i,]
1582897379680:xm <- scale(x, scale=F)
1582897379681:sum(xm^2)
1582897379682:}
1582897379683:})
1582897379683:aveD <- tapply(1:nrow(dat),z,function(i){
1582897379684:if (length(i)==1) 0
1582897379685:else {x<- dat[i,]
1582897379686:xm <- scale(x, scale=F)
1582897379687:xm <- apply(xm, 1, function(y) sqrt(sum(y^2)))
1582897379688:mean(xm)
1582897379689:}
1582897379690:})
1582897379692:maxD <- tapply(1:nrow(dat),z,function(i){
1582897379693:if (length(i)==1) 0
1582897379694:else {x<- dat[i,]
1582897379695:xm <- scale(x, scale=F)
1582897379696:xm <- apply(xm, 1, function(y) sqrt(sum(y^2)))
1582897379697:max(xm)
1582897379697:}
1582897379698:})
1582897379700:part<- data.frame("N.obs"=nobs, "Within clus SS" = withinSS, "Ave dist  Centroid" = aveD,
1582897379701:"Max dist centroid" =maxD)
1582897379702:rownames(part)<- paste("Cluster", 1:k)
1582897379706:cCen <- cbind(simplify2array(tapply(1:nrow(dat),z,function(i){
1582897379707:x<- dat[i,]
1582897379708:if (length(i)==1) x
1582897379709:else {
1582897379710:colMeans(x)
1582897379711:}
1582897379712:})), colMeans(dat))
1582897379713:colnames(cCen)<- c(paste("Cluster", 1:k), "Grand centrd")
1582897379715:cCenD <- as.matrix(dist(t(cCen[, -ncol(cCen)])))
1582897379716:cat("Final Partition\n")
1582897379717:cat("\n")
1582897379718:cat(paste("Number of clusters ", k))
1582897379719:cat("\n")
1582897379721:cat("\n")
1582897379722:print(part, quote=F)
1582897379723:cat("\n")
1582897379724:cat("\n")
1582897379725:cat("Cluster centroids\n")
1582897379726:cat("\n")
1582897379727:print(cCen,quote=F)
1582897379728:cat("\n")
1582897379729:cat("\n")
1582897379730:cat("Distances between Cluster centroids\n")
1582897379731:cat("\n")
1582897379732:print(cCenD,quote=F)
1582897379733:list(withinSS=withinSS, aveD=aveD, centroids=cCen, centroidDist=cCenD)
1582897379734:}
1582897379743:sumPartition(scorescutree(avcl,3))
1582897412836:library(dendextend)
1582897412838:avcld2 <- color_branches(avcld,k=3,col=c(2,3,5))
1582897412949:plot(avcld2)
1582897413032:plot(avcld2,xlim = c(1,30),ylim = c(0,1.5))
1582897413169:cutree(avcl,3)
