1582902425937:screeplot <- function(p) {
1582902425938:e <- p$sdev ^ 2
1582902425939:e <- e / sum(e)
1582902425940:plot(
1582902425941:1:length(e),
1582902425942:e,
1582902425942:xlab = "Component number",
1582902425943:pch = 20,
1582902425944:ylab = "Variance proportion",
1582902425945:main = "Scree plot",
1582902425946:axes = F,
1582902425947:ylim = c(0, max(e)*1.04)
1582902425947:)
1582902425948:lines(1:length(e), e)
1582902425949:axis(1, at = 1:length(e))
1582902425950:axis(2)
1582902425951:}
1582902425956:sumPartition <- function(dat,z){
1582902425957:k <-length(unique(z))
1582902425958:nobs <- tapply(1:nrow(dat),z,length)
1582902425958:withinSS <- tapply(1:nrow(dat),z,function(i){
1582902425959:if (length(i)==1) 0
1582902425960:else {x<- dat[i,]
1582902425961:xm <- scale(x, scale=F)
1582902425962:sum(xm^2)
1582902425963:}
1582902425964:})
1582902425965:aveD <- tapply(1:nrow(dat),z,function(i){
1582902425965:if (length(i)==1) 0
1582902425966:else {x<- dat[i,]
1582902425967:xm <- scale(x, scale=F)
1582902425968:xm <- apply(xm, 1, function(y) sqrt(sum(y^2)))
1582902425969:mean(xm)
1582902425970:}
1582902425971:})
1582902425973:maxD <- tapply(1:nrow(dat),z,function(i){
1582902425973:if (length(i)==1) 0
1582902425974:else {x<- dat[i,]
1582902425975:xm <- scale(x, scale=F)
1582902425976:xm <- apply(xm, 1, function(y) sqrt(sum(y^2)))
1582902425977:max(xm)
1582902425978:}
1582902425979:})
1582902425981:part<- data.frame("N.obs"=nobs, "Within clus SS" = withinSS, "Ave dist  Centroid" = aveD,
1582902425982:"Max dist centroid" =maxD)
1582902425983:rownames(part)<- paste("Cluster", 1:k)
1582902425986:cCen <- cbind(simplify2array(tapply(1:nrow(dat),z,function(i){
1582902425987:x<- dat[i,]
1582902425988:if (length(i)==1) x
1582902425989:else {
1582902425990:colMeans(x)
1582902425991:}
1582902425992:})), colMeans(dat))
1582902425993:colnames(cCen)<- c(paste("Cluster", 1:k), "Grand centrd")
1582902425995:cCenD <- as.matrix(dist(t(cCen[, -ncol(cCen)])))
1582902425996:cat("Final Partition\n")
1582902425997:cat("\n")
1582902425998:cat(paste("Number of clusters ", k))
1582902425999:cat("\n")
1582902426001:cat("\n")
1582902426002:print(part, quote=F)
1582902426003:cat("\n")
1582902426004:cat("\n")
1582902426005:cat("Cluster centroids\n")
1582902426006:cat("\n")
1582902426007:print(cCen,quote=F)
1582902426008:cat("\n")
1582902426009:cat("\n")
1582902426010:cat("Distances between Cluster centroids\n")
1582902426011:cat("\n")
1582902426012:print(cCenD,quote=F)
1582902426013:list(withinSS=withinSS, aveD=aveD, centroids=cCen, centroidDist=cCenD)
1582902426014:}
